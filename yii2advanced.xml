  <SqlTexts>
    <SqlText path="">
      <Sql>select u.id,u.username as 'Usuario', u.nombre_completo as 'Nombre Completo', u.email as 'Correo Electronico', &#13;
	case when u.estado=0 then 'Inactivo' else &#13;
    case when u.estado=1 then 'Activo' else 'Activado' end end as Estado&#13;
from users u join users p on u.patrocinador=p.id where p.id=66;&#13;
select u.id, u.username as Usuario, &#13;
	u1.id, u1.username as P1, u2.id, u2.username as P2, u3.id, u3.username as P3, u4.id, u4.username as P4, u5.id, u5.username as P5, u6.id, u6.username as P6 &#13;
from users u&#13;
left join users u1 on u.patrocinador=u1.id&#13;
left join users u2 on u1.patrocinador=u2.id&#13;
left join users u3 on u2.patrocinador=u3.id&#13;
left join users u4 on u3.patrocinador=u4.id&#13;
left join users u5 on u4.patrocinador=u5.id&#13;
left join users u6 on u5.patrocinador=u6.id&#13;
where u.id=701;</Sql>
    </SqlText>
  </SqlTexts>
